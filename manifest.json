{
    "manifest_version": 2,

    "name": "Click Cash",
    "description": "Clicks pay with cash option in delivery.com's referral",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": ["https://www.delivery.com/*"],
            "js": ["popup.js"]
         }
     ]
}
